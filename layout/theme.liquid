  <link rel="preconnect" href="{{ 'Romie-Italic.woff' | file_url }}" as="font" type="font/woff" crossorigin>
  <link rel="preconnect" href="{{ 'Romie-Regular.woff2' | file_url }}" as="font" type="font/woff2" crossorigin>
  <link rel="preconnect" href="{{ 'Romie-Regular.woff' | file_url }}" as="font" type="font/woff" crossorigin>
  <link rel="preconnect" href="{{ 'gotham-light-webfont.woff2' | file_url }}" as="font" type="font/woff2" crossorigin>
  <link rel="preconnect" href="{{ 'gotham-light-webfont.woff' | file_url }}" as="font" type="font/woff" crossorigin>
  <link rel="preconnect" href="{{ 'gotham-book-webfont.woff2' }}" as="font" type="font/woff2" crossorigin>
