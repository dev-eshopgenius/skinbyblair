    "495c5033-7b1f-4165-ac43-b0599ca4ff51": {
      "type": "page_content",
      "blocks": {
        "template--14654690558036__495c5033-7b1f-4165-ac43-b0599ca4ff51-text-1": {
          "type": "text",
          "settings": {
            "text": "<h2>Squalane<\/h2><p>An antioxidant that locks in moisture, soothes inflammation, and supports the skin barrier<\/p><h2><\/h2><h2>Caffeine<\/h2><p>A natural antixodiant that reduces puffiness and improves skin circulation<\/p><h2><\/h2><h2>Ceramides<\/h2><p>Essential lipids that strengthen the skin barrier to retain moisture, and improve visible signs of aging<\/p><h2><\/h2><h2>Cholesterol<\/h2><p>Work to assist ceramides in order to maximize moisture retention & strength the skin barrier<\/p><p><\/p><p><\/p><p><\/p><p><\/p><p><\/p>",
            "text_style": "font-body"
          }
        }
      },
