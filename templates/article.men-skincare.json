        "text_h86Yyq": {
          "type": "text",
          "settings": {
            "text": "<h2>Everything he needs: simple yet effective products<\/h2>",
            "text_size": 4,
            "text_style": "font-body"
          }
        }
      },
      "block_order": [
        "text_h86Yyq"
