    "e10604ee-2959-4cfc-b98b-8fc2b04687d7": {
      "type": "section-custom-richtext",
      "blocks": {
        "template--14705319870548__e10604ee-2959-4cfc-b98b-8fc2b04687d7-subheading-1": {
          "type": "subheading",
          "disabled": true,
          "settings": {
            "subheading": "Introducing",
            "text_size": 3,
            "text-align-class": "hero-text-left",
