    "section_custom_standard_double_aFEWKJ": {
      "type": "section-custom-standard-double",
      "blocks": {
        "heading_mV7JLf": {
          "type": "heading",
          "settings": {
            "title": "Must Haves",
            "title2": "Must Haves",
            "text_size": 8
          }
        },
        "text_MJ6NYg": {
          "type": "text",
          "settings": {
            "text": "<p><\/p><p>• Current licensure as a Physician Assistant (PA-C),  Nurse Practitioner (NP), or Registered Nurse (RN) in the state of North Carolina<\/p><p><br\/>• Minimum of 3 years of experience performing aesthetic injections in a clinical setting<\/p><p><br\/>• Strong knowledge of facial anatomy and experience with injectable techniques<\/p><p><br\/>• Excellent communication and interpersonal skills with a focus on providing exceptional patient care<\/p><p><br\/>• Ability to work independently as well as collaboratively within a team environment<\/p><p><br\/>• Commitment to upholding the highest standards of professionalism, ethics, and safety<\/p><p><\/p>",
            "text_size": 5,
            "text_style": "font-body"
          }
        },
        "buttons_EwQdrq": {
