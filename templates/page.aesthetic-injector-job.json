    "section_custom_standard_double_qT8VAh": {
      "type": "section-custom-standard-double",
      "blocks": {
        "heading_wFAF9G": {
          "type": "heading",
          "settings": {
            "title": "The Position",
            "title2": "The Position",
            "text_size": 8
          }
        },
        "text_dUjFrq": {
          "type": "text",
          "settings": {
            "text": "<p><\/p><p>• Conduct comprehensive consultations to assess patient needs and develop personalized treatment plans<\/p><p><br\/>• Provide thorough education to patients on treatment procedures, including pre- and post-care instructions, ensuring informed decision-making<\/p><p><br\/>• Administer injectable treatments including neurotoxins, dermal fillers, and other advanced injectables with attention to detail and precision, ensuring the highest standards of patient safety and comfort<\/p><p><br\/>• Prioritize caution when considering external factors or individual patient variables that may influence treatment safety or desired outcomes<\/p><p><br\/>• Maintain accurate and detailed patient records and documentation of information related to treatments, outcomes, and medical history<\/p><p><br\/>• Adhere to all federal and state regulations governing medical practices to maintain the highest level of patient safety and privacy<\/p><p><br\/>• Stay informed about industry trends, new products, and techniques through continuing education and training<\/p><p><br\/>• Consistently demonstrate professionalism when interacting with patients and team members, fostering a positive and respectful environment within the team and instilling confidence in patients<\/p><p><br\/>• Collaborate effectively with other team members to deliver seamless client experiences<\/p>",
            "text_size": 5,
            "text_style": "font-body"
          }
        },
        "buttons_xjBAXk": {
          "type": "buttons",
          "disabled": true,
          "settings": {
            "button_text": "Learn more",
            "button_url": "",
            "button_type": "btn--primary",
