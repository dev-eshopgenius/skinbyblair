        },
        "text_MJ6NYg": {
          "type": "text",
          "settings": {
            "text": "<p><br\/>• Bachelor’s Degree or comparable experience equivalency<br\/><br\/>• 3+ years of experience in digital marketing, preferably within the e-commerce sector<br\/><br\/>• Proficient in e-commerce platforms (Shopify), and digital marketing tools (Google Analytics, Google & Meta Ads), and social media management platforms<br\/><br\/>• Experience using storytelling techniques to create compelling and memorable brand experiences that drive customer loyalty and revenue growth<br\/><br\/>• Outstanding copywriting, proofreading, and communication skills with a high level of creativity and attention to detail<br\/><br\/>• Passionate about brand development and growth strategies, with a strong analytical mindset<br\/><br\/>• Highly curious, organized, resourceful, and eager to learn<br\/><br\/>• Proactive and able to independently move projects forward, prioritize tasks, and meet deadlines<br\/><br\/>• Positive and upbeat personality<\/p>",
            "text_size": 5,
            "text_style": "font-body"
          }
        },
