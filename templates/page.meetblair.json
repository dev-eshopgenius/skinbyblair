        "heading_size": 7,
        "description": "",
        "text_size": 3,
        "use_richtext": true,
        "richtext": "<p><strong>OUR MISSION<\/strong><\/p><h3>We help women transform their skin,<br\/>encourage self-care, and build self-confidence that radiates from the inside out.<\/h3>",
        "flex_align": "align--middle-center",
