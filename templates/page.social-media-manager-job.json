    "section_custom_standard_double_qT8VAh": {
      "type": "section-custom-standard-double",
      "blocks": {
        "heading_wFAF9G": {
          "type": "heading",
          "settings": {
            "title": "The Position",
            "title2": "The Position",
            "text_size": 8
          }
        },
        "text_dUjFrq": {
          "type": "text",
          "settings": {
            "text": "<p><br\/>• Create and edit unique, innovative, and educational video and blog content for social channels, ensuring a consistent voice across all platforms to grow social engagement<br\/><br\/>• Expertly manage social platforms including Instagram, TikTok, YouTube, and Pinterest<br\/><br\/>• Create and maintain a strategic monthly content calendar, including copy and scheduling of daily posts across channels<br\/><br\/>• Research and brainstorm new topics and creative ways to share across channels<br\/><br\/>• Organize and strategically repurpose current video and written content<br\/><br\/>• Conduct market research and identify trends<br\/><br\/>• Evaluate and monitor campaign performance on an ongoing basis by analyzing key metrics and creating comprehensive reports<\/p>",
            "text_size": 5,
            "text_style": "font-body"
          }
        },
