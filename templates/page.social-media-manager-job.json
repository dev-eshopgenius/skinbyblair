    "section_custom_standard_double_aFEWKJ": {
      "type": "section-custom-standard-double",
      "blocks": {
        "heading_mV7JLf": {
          "type": "heading",
          "settings": {
            "title": "Must Haves",
            "title2": "Must Haves",
            "text_size": 8
          }
        },
        "text_MJ6NYg": {
          "type": "text",
          "settings": {
            "text": "<p><br\/>• Prior social media marketing experience<br\/><br\/>• Passionate about social media and the aesthetics industry<br\/><br\/>• Expert knowledge on various social platforms, including Instagram, TikTok, YouTube, and Pinterest<br\/><br\/>• Highly creative, pay excellent attention to detail, and have a refined aesthetic eye<br\/><br\/>• Strong video editing skills and proficiency in Final Cut Pro or similar video editing software<br\/><br\/>• Outstanding copy-writing and proofreading skills, as well as excellent written and verbal communication skills<br\/><br\/>• Strong knowledge of aesthetics or willingness to learn<br\/><br\/>• Highly curious, extremely resourceful, and use credible sources when researching topics<br\/><br\/>• Eager to constantly learn and expand knowledge of skin, science, and aesthetics<br\/><br\/>• Able to accept feedback, take suggestions, and be eager to improve and grow<br\/><br\/>• Proactive and able to independently move projects forward, prioritize tasks, and meet deadlines<br\/><br\/>• Highly organized and dependable, have an eye for detail, and have strong analytical skills<br\/><br\/>• Motivated individual with the ability to multitask, work independently, and be open to learning and ready to “jump-in” to get the job done<br\/><br\/>• Positive and upbeat personality<\/p>",
            "text_size": 5,
            "text_style": "font-body"
          }
        },
